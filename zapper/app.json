{
  "expo": {
    "name": "zapper",
    "slug": "zapper",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Zapper to use your location."
        }
      ],
      "expo-secure-store"
    ],
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff",
        "package": "com.zapper.pti"
      },
      "package": "com.zapper.pti",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyD2BPWjmF2OT6nSF6NETB4HHuWOJk1t8Sg"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
